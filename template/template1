#是否添加头文件的doxygen注释
DOXYGEN : No

#相邻变量定义的空行数
VARIABLE_INTERVAL : 1

#相邻函数定义的空行数
FUNCTION_INTERVAL : 2

#变量定义生成格式
VARIABLE : " TYPE\tFULLNAME; "

#函数定义生成格式
FUNCTION : "

TYPE FULLNAME(TYPE) 
{

} 

"

#以下皆为可选项
#变量定义开始处添加描述
VARIABLE_START : ""

#变量定义结束处添加描述
VARIABLE_END : ""

#函数定义开始处添加描述
FUNCTION_START : ""

#函数定义结束处添加描述
FUNCTION_END : ""

#类定义开始处添加描述
CLASS_START : ""

#类定义结束处添加描述
CLASS_END : ""

#头文件开始处添加描述
HEADER_START : "

/* 
 * Auto generate by h2cppx
 */

"

#头文件处添加描述
HEADER_END : ""


