{

} 

"

#以下皆为可选项
#变量定义开始处添加描述
VARIABLE_START : ""

#变量定义结束处添加描述
VARIABLE_END : ""

#函数定义开始处添加描述
FUNCTION_START : ""

#函数定义结束处添加描述
FUNCTION_END : ""

