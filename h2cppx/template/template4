#相邻变量定义的空行数
VARIABLE_INTERVAL : 2

#相邻函数定义的空行数
FUNCTION_INTERVAL : 2

#变量定义生成格式
VARIABLE : "
//variable $variable.owner in $variable.owner
$variable.sign_type $variable.sign_name;
" 

#函数定义生成格式
FUNCTION : "

#if $function.constructor:
/* constructor method in $function.path */
#else if $function.destructor:
/* destructor method in $function.path */
#else if $function.access: 
/* $function.access #slurp
#if $function.virtual:
virtual #slurp
#end if
method in $function.path */
#else
/* global function */
#end if
#if not $function.constructor and not $function.destructor:
${function.return_type} #slurp
#end if
${function.sign_name}(#slurp
#for $p in $function.parameters:
$p.type $p.name#slurp
#if $p.default_value
 /* =$p.default_value */ #slurp
#end if
#if $function.parameters.index($p)!=$len($function.parameters)-1
, #slurp
#end if
#end for 
) 
{
#if $function.return_type == 'void':

#else if $function.returns in ['int','unsigned int','long','unsigned long','char','unsigned char']:
    return 0;
#else if $function.returns == 'bool'
    return false;
#else if $function.returns == 'float': 
    return 0.0f;
#else if $function.returns == 'double':
    return 0.0;
#else
    return ${function.returns}();
#end if
} 

"

#以下皆为可选项
#变量定义开始处添加描述
VARIABLE_START : ""

#变量定义结束处添加描述
VARIABLE_END : ""

#函数定义开始处添加描述
FUNCTION_START : ""

#函数定义结束处添加描述
FUNCTION_END : ""

#类定义开始处添加描述
CLASS_START : ""

#类定义结束处添加描述
CLASS_END : ""

